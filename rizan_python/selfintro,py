print(b)
print(type(b))

c = "i lived in tvm kerala"
print(c)
print(type(c))

d = "i have a family of 3"
print(d)
print(type(d))

e = "i have done my bachelors degree in b.com degree from kerala university trivandrum"
print(e)
print(type(e))

f = "i have got experience in accounts for 1 year"
print(f)
print(type(f))

g = "my strengths are im hardworking"
